{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"],
        "components/*": ["src/components/*"],
        "assets/*": ["src/assets/*"],
        "views/*": ["src/views/*"],
        "common/*": ["src/common/*"],
        "network/*": ["src/network/*"]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
